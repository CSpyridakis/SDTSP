ls /tmp/
ls -al /tmp/
touch file | cat file
end
seq 1 1 10 | grep -c 1
ls /tmp/ ; echo "Hello"
end 
ls -al /tmp/ | tr -s " "  
ls -al /tmp/ | tr -s " " | cut -d" " -f1
cat /etc/passwd | grep -c ":"
ls -al /etc | cat | tr -s " " | grep "root" | cut -d" " -f3,4
echo "Hello"

;

ls -alR /etc/ | touch file
timeToStop